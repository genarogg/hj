
{
  "name": "genarogg-pg2-alberto",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "scss": "sass --watch public/css/sass/style.scss public/css/style.css",
    "browser-sync": "browser-sync start -p localhost:3000  --files='./views/*.ejs, ./public/js/*.js , ./public/css/sass/style.scss' --no-ui",
    "develop": "cls && concurrently \"npm run dev\" \"npm run browser-sync\"  \"npm run scss\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "sass": "^1.62.0"
  },
  "devDependencies": {
    "browser-sync": "^2.29.1",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
